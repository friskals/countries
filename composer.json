{
    "name": "friskals/countries",
    "description": "Nominal and currency formatting for Laravel Framework",
    "type": "library",
    "keywords": [
        "format-currency",
        "format-number",
        "country"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Friska Sianturi",
            "email": "friskasianturi23@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Friskals\\Countries\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Friskals\\Countries\\Providers\\CountryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}